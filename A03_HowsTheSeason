        var calculationsUrl = '/A03_HowsTheSeason/Calculate';


        $(function() {
            initLocationSelector();
            generateInputsUI();
            generateOutputsUI();
            calculate(mydatasource, calculationsUrl);
        });


        function generateInputsUI() {
            addQuestion("Question1");
            //Row1 ******************************
            var row = 1;
            addInputLabel("DataType", row, 2,2);
            addInputEditor("DataType", row, 10,10);
            //Row2 ******************************
            ++row;
            addInputLabel("Location", row, 2,2);
            addInputEditor("Location", row, 10,10);
            //Row3 ******************************
            ++row;
            addInputLabel("MonitorPeriod", row, 6,6);
            addInputEditor("MonitorPeriod", row, 6,6);
            //Row4 ******************************
            ++row;
            addInputLabel("PredictionPeriod", row, 6,6);
            addInputEditor("PredictionPeriod", row, 6,6);
            //Row ******************************
            ++row;
            addInputLabel("YearsRange", row, 6,6);
            addInputEditor("YearsRange", row, 6,6);


            //Change Events *********************
            $(".input-control").change(function() {
              
                if (updateInputValue(mydatasource, $(this))) {
                    calculate(mydatasource, calculationsUrl);
                }
            });
        }

        function generateOutputsUI() {
            addOutput("Question1");
            addOutput("Answer1");
            addOutput("Answer2");
            addOutput("FireGauge1");
            addOutput("Chart1");
            addOutput("HowDoWeKnowThis");
            var bodyWidth = $("body").width();
            if (bodyWidth < 700) {
                $("#Chart1 .flotchart-canvas").css("height", "260px");
                
            }
        }

    </script>


    <script type="text/javascript">
        $(function() {
            var $metismenu = $("#analysis-metismenu");
            var $selectedli = $(".analysis-a3");
            var $parentlis = $selectedli.parents("li");
            $selectedli.addClass("active");
            $selectedli.addClass("selected");
            $parentlis.addClass("active");
            $metismenu.metisMenu();
        });
        window.addEventListener('resize', posFix, false);

        function posFix() {
            generateOutputsUI();

        }

        function sendFeedback() {
            openFeedbackDialog("HowsTheSeason", "db189a9f-8b01-46ee-b5a8-76b375f4c572");
        }

        function getHelp() {
            $('.ajax_spinner').show();
            $.getJSON("/Analyses/GetHelp",null, function (results) {
                if (results.success) {
                    $("#help-title").text(results.title);
                    if (results.image && results.image.length > 0) {
                        $("#help-image").attr('src', results.image);

                    } else {
                        $("#help-image").hide();
                    }
                    $("#help-content").html(results.content);
                    $('.ajax_spinner').hide();
                    openNav();
                } else {
                    $('.ajax_spinner').hide();
                    custom_alert("Sorry - couldn't find any help for this analysis.", "Help not at hand...");
                }
                
            });
        }
        function openNav() {
           
            //$("#myNav").css("width", "100%");
            $("#myNav").css("left", "0");
        }

        function closeHelp() {
            //$("#myNav").css("width","0%");
            $("#myNav").css("left", "-100%");
        }

        function custom_alert(message, title) {
            if (!title)
                title = 'Alert';

            if (!message)
                message = 'No Message to Display.';

            $('<div></div>').html(message).dialog({
                title: title,
                resizable: false,
                modal: true,
                buttons: {
                    'Ok': function () {
                        $(this).dialog('close');
                    }
                }
            });
        }

    </script>


