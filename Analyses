        var calculationsUrl = '/A10_WhatTrend/Calculate';

        $(function() {
            initLocationSelector();
            generateInputsUI();
            generateOutputsUI();
            calculate(mydatasource, calculationsUrl);
        });

        function generateInputsUI() {
            addQuestion("Question1");
            //Row ******************************
            var row = 1;

            addInputLabel("DataType", row, 1,3);
            addInputEditor("DataType", row, 5,9);
            addInputLabel("Location", row, 1,3);
            addInputEditor("Location", row, 5,9);
            //Row ******************************
            ++row;
            addInputLabel("YearsRange", row, 1,3);
            addInputEditor("YearsRange", row, 5,9);
            addInputLabel("MonthsRange", row, 1,3);
            addInputEditor("MonthsRange", row, 5,9);


            //Row ******************************
            ++row;
            addInputLabel("EqualityType", row, 1,3);
            addInputEditor("EqualityType", row, 2,4);
            addInputEditor("RainfallThreshold", row, 3,5); // will be hidden when inactive
            addInputEditor("MaxTempThreshold", row, 3,5); // will be hidden when inactive
            addInputEditor("MinTempThreshold", row, 3,5); // will be hidden when inactive
            addInputEditor("RadiationThreshold", row, 3,5); // will be hidden when inactive
            addInputLabel("Period", row, 1,3);
            addInputEditor("Period", row, 5,9);
            //*************************************

            hideShowThresholdSelectors();
            //Change Events *********************
            $(".input-control").change(function() {
                if (updateInputValue(mydatasource, $(this))) {
                    hideShowThresholdSelectors();
                    refeshPeriodSelector();
                    calculate(mydatasource, calculationsUrl);
                }
            });
        }

        function generateOutputsUI() {
            addOutput("Question1");
            addOutput("Answer1");
            addOutput("ChartsTitle1");

            // addOutput("FireGauge1");
            addOutput("Chart1");
            addOutput("Chart2");
            addOutput("Chart3");
            if (mydatasource.Outputs && mydatasource.Outputs["Chart4"]) {
                $("#ChartsTitle2").show();
                $("#Chart4").show();
                addOutput("Chart4");
                addOutput("ChartsTitle2");
            } else {
                $("#ChartsTitle2").hide();
                $("#Chart4").hide();
            }
            addOutput("HowDoWeKnowThis");
            addAveragingPeriodInput();

            var bodyWidth = $("body").width();
            if (bodyWidth < 700) {
                $("#Chart1 .flotchart-canvas").css("height", "200px");
                $("#Chart2 .flotchart-canvas").css("height", "200px");
                $("#Chart3 .flotchart-canvas").css("height", "200px");
                $("#Chart4 .flotchart-canvas").css("height", "200px");
            }
        }

        function addAveragingPeriodInput() {
            var identifier = "AveragingPeriod";
            var settings = mydatasource.InputConfig[identifier];
            var modelvalue = mydatasource.InputValues[identifier];
            var html = "<select  class='form-control input-control'  id='" +
                identifier +
                "'' style='max-width:60px;display:inline;padding:2px'>";
            for (var i = 0; i < settings.Items.length; ++i) {
                var item = settings.Items[i];
                html = html + "<option value='" + item.Value + "'>" + item.Key + "</option>";
            }

            html = html + "</select>";
            $("#" + identifier + "-container").html(html);
            $("#" + identifier).val(modelvalue);
            $("#" + identifier).change(function() {
                updateInputValue(mydatasource, $(this));
                hideShowThresholdSelectors();
                refeshPeriodSelector();
                calculate(mydatasource, calculationsUrl);
            });
        }

        function refeshPeriodSelector() {
            var inputs = mydatasource.InputValues;
            var periodSize = getPeriodLimit();
            if (inputs.Period > periodSize)
                inputs.Period = periodSize;
            rebuildPeriodSelector("Period", inputs.Period, 1, periodSize);
        }

        function getPeriodLimit() {
            var inputs = mydatasource.InputValues;
            var months = inputs.MonthsRange % 12 + 1;
            var days = months * 30;
            return days;

        }


        function GetPeriodLength(monthsRange) {
            return monthsRange % 12 + 1;
        }


        function hideShowThresholdSelectors() {
            var datatype = Number(mydatasource.InputValues.DataType);
            hideShowElement("RainfallThreshold", datatype === 11);
            hideShowElement("MaxTempThreshold", datatype === 12);
            hideShowElement("MinTempThreshold", datatype === 13);
            hideShowElement("RadiationThreshold", datatype === 14);
            hideShowElement("EqualityType", datatype > 9);
            hideShowElement("Period", datatype > 9);
        }


    </script>


    <script type="text/javascript">
        $(function() {
            var $metismenu = $("#analysis-metismenu");
            var $selectedli = $(".analysis-a10");
            var $parentlis = $selectedli.parents("li");
            $selectedli.addClass("active");
            $selectedli.addClass("selected");
            $parentlis.addClass("active");
            $metismenu.metisMenu();
        });
        window.addEventListener('resize', posFix, false);

        function posFix() {
            generateOutputsUI();

        }

        function sendFeedback() {
            openFeedbackDialog("WhatTrend", "413383aa-ac65-4243-8831-9ec58edafcd1");
        }

        function getHelp() {
            $('.ajax_spinner').show();
            $.getJSON("/Analyses/GetHelp",null, function (results) {
                if (results.success) {
                    $("#help-title").text(results.title);
                    if (results.image && results.image.length > 0) {
                        $("#help-image").attr('src', results.image);

                    } else {
                        $("#help-image").hide();
                    }
                    $("#help-content").html(results.content);
                    $('.ajax_spinner').hide();
                    openNav();
                } else {
                    $('.ajax_spinner').hide();
                    custom_alert("Sorry - couldn't find any help for this analysis.", "Help not at hand...");
                }
                
            });
        }
        function openNav() {
           
            //$("#myNav").css("width", "100%");
            $("#myNav").css("left", "0");
        }

        function closeHelp() {
            //$("#myNav").css("width","0%");
            $("#myNav").css("left", "-100%");
        }

        function custom_alert(message, title) {
            if (!title)
                title = 'Alert';

            if (!message)
                message = 'No Message to Display.';

            $('<div></div>').html(message).dialog({
                title: title,
                resizable: false,
                modal: true,
                buttons: {
                    'Ok': function () {
                        $(this).dialog('close');
                    }
                }
            });
        }

    </script>


