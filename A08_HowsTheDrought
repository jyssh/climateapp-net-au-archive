        var fullanalysis = mydatasource.InputValues.RunFullDroughtAnalysis;
        var calculationsUrl = '/A08_HowsTheDrought/Calculate';


        $(function() {
            initLocationSelector();
            addInputsAnswerHtml();
            generateInputsUI();
            generateOutputsUI();
            calculate(mydatasource, calculationsUrl);
        });

        function generateInputsUI() {
            addQuestion("Question1");
            //Row1 ******************************
            var row = 1;
            
            
            if (mydatasource.InputValues.RunFullDroughtAnalysis) {
                addInputLabel("Location", row, 1,4);
                addInputEditor("Location", row, 3,8);
                addInputLabel("DroughtType", row, 2,4);
                addInputEditor("DroughtType", row, 2,8);
                addInputLabel("RankType", row, 1, 4);
                addInputEditor("RankType", row, 3, 8);
                //Row1 ******************************
                ++row;
                
                addInputLabel("EnsoType", row, 5, 4);
                addInputEditor("EnsoType", row, 3, 8);
                addInputLabel("YearsRange", row, 1, 4);
                addInputEditor("YearsRange", row, 3, 8);
            } else {
                addInputLabel("Location", row, 4,4);
                addInputEditor("Location", row, 8,8);
                ++row;
                addInputLabel("DroughtType", row, 4,4);
                addInputEditor("DroughtType", row, 8,8);
                ++row;
                addInputLabel("RankType", row, 4,4);
                addInputEditor("RankType", row, 8,8);
                ++row;
                addInputLabel("YearsRange", row, 4,4);
                addInputEditor("YearsRange", row, 8,8);    
            }
            ++row;
            
            //Change Events *********************
            $(".input-control").change(function() {
                if (updateInputValue(mydatasource, $(this))) {
                    calculate(mydatasource, calculationsUrl);
                }
            });
        }


        function addInputsAnswerHtml() {
            var html = "";
            if (fullanalysis) {
                html = html + "<div id='advanced-inputs'>";
                html = html + "     <div class='row'>";
                html = html + "          <div class='col-md-12'>";
                html = html + "               <div class='question-panel' id='analysis-inputs'></div>";
                html = html + "          </div>";
                html = html + "     </div>";
                html = html + "     <div class='row'>";
                html = html + "          <div class='col-md-12'>";
                html = html + "               <div class='answer-panel'>";
                html = html + "                    <div class='row'>";
                html = html + "                         <div class='col-md-12'>";
                html = html + "                              <div class='answer' id='Answer1'></div>";
                html = html + "                         </div>";
                html = html + "                    </div>";
                html = html + "                    <div class='row'>";
                html = html + "               <div class='col-sm-6'>";
                html = html + "                    <div class='firegauge-container' id='FireGauge1'></div>";
                html = html + "               </div>";
                html = html + "               <div class='col-sm-6'>";
                html = html + "                    <div id='Report' style='margin-top: 6px;'></div>";
                html = html + "               </div>";
                html = html + "          </div>";
                html = html + "     </div>";
                html = html + "</div>";
                html = html + "</div>";
                html = html + "</div>";
            } else {
                html = html + "<div id='basic-inputs' class='question-container-8' >";
                html = html + "     <div class='col-sm-6 equalise'>";
                html = html + "          <div class='question-panel' id='analysis-inputs' style='height: 100%'></div>";
                html = html + "     </div>";
                html = html + "     <div class='col-sm-6 equalise'>";
                html = html + "          <div class='answer-panel' style='margin-top: auto; margin-bottom: auto; height: 100%;'>";
                html = html + "               <div class='answer' id='Answer1'></div>";
                html = html + "               <div class='firegauge-container' id='FireGauge1' style='margin-top: 15px'></div>";
                html = html + "          </div>";
                html = html + "     </div>";
                html = html + "</div>";
            }
            $("#inputs-answer-panel").html(html);
        }

        function generateOutputsUI() {
            addOutput("Question1");
            addOutput("Answer1");
            addOutput("Report");
            addOutput("FireGauge1");
            var plot1=addOutput("Chart1");
            addOutput("Chart2");
            addOutput("Table1");
            addOutput("HowDoWeKnowThis");
           hideShowTableColums();
            appendChartAnnotations(plot1);
            $("#FireGauge1").css("margin-top", "0px");
            var bodyWidth = $("body").width();
            if (bodyWidth < 700) {
                $("#Chart1 .flotchart-canvas").css("height", "260px");
                $("#Chart2 .flotchart-canvas").css("height", "260px");
            }

        }
        function appendChartAnnotations(plot1) {
            var centerx;
            var o1, o2;
            var selector1= "#Chart1 .flotchart-canvas";
            centerx = $(selector1).width() / 2.0-50;
            o1 = plot1.pointOffset({ x: 2, y: 30 });
            $(selector1).append('<div style="position:absolute;left:' + (70) + 'px;top:' + (o1.top) + 'px;color:white;font-size:16px;opacity:0.8">Warning Zone</div>');
            o2 = plot1.pointOffset({ x: 2, y: 10 });
            $(selector1).append('<div style="position:absolute;left:' + (70) + 'px;top:' + (o2.top) + 'px;color:white;font-size:16px;opacity:0.8">Drought Zone</div>');

            
        }

        function hideShowTableColums() {
            //var rank = Number(mydatasource.InputValues["RankType"]);
            //(rank === 0) ? $(".col3").show() : $(".col3").hide();
            //(rank === 1) ? $(".col2").show() : $(".col2").hide();
            //(rank === 2) ? $(".col4").show() : $(".col4").hide();
            var bodyWidth = $("body").width();
            var width = (bodyWidth < 600) ? bodyWidth : 600;

            if (bodyWidth < 600) {
                $(".col5").hide();
                $(".col6").hide();
                $(".col7").hide();
                $(".col8").hide();
                $(".col9").hide();
            } else {
                $(".col5").show();
                $(".col6").show();
                $(".col7").show();
                $(".col8").show();
                $(".col9").show();
                
            }

        }

    </script>


    <script type="text/javascript">
        $(function() {
            var $metismenu = $("#analysis-metismenu");
            var $selectedli = $(".analysis-a8");
            var $parentlis = $selectedli.parents("li");
            $selectedli.addClass("active");
            $selectedli.addClass("selected");
            $parentlis.addClass("active");
            $metismenu.metisMenu();
        });
        window.addEventListener('resize', posFix, false);

        function posFix() {
            generateOutputsUI();

        }

        function sendFeedback() {
            openFeedbackDialog("HowsTheDrought", "e7a45f7c-6487-4eee-b3fc-387fef35dd67");
        }

        function getHelp() {
            $('.ajax_spinner').show();
            $.getJSON("/Analyses/GetHelp",null, function (results) {
                if (results.success) {
                    $("#help-title").text(results.title);
                    if (results.image && results.image.length > 0) {
                        $("#help-image").attr('src', results.image);

                    } else {
                        $("#help-image").hide();
                    }
                    $("#help-content").html(results.content);
                    $('.ajax_spinner').hide();
                    openNav();
                } else {
                    $('.ajax_spinner').hide();
                    custom_alert("Sorry - couldn't find any help for this analysis.", "Help not at hand...");
                }
                
            });
        }
        function openNav() {
           
            //$("#myNav").css("width", "100%");
            $("#myNav").css("left", "0");
        }

        function closeHelp() {
            //$("#myNav").css("width","0%");
            $("#myNav").css("left", "-100%");
        }

        function custom_alert(message, title) {
            if (!title)
                title = 'Alert';

            if (!message)
                message = 'No Message to Display.';

            $('<div></div>').html(message).dialog({
                title: title,
                resizable: false,
                modal: true,
                buttons: {
                    'Ok': function () {
                        $(this).dialog('close');
                    }
                }
            });
        }

    </script>


