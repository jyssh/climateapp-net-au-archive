        var calculationsUrl = '/A09_WhatYield/Calculate';


        $(function() {
            initLocationSelector();
            generateInputsUI();
            generateOutputsUI();
            calculate(mydatasource, calculationsUrl);
        });

        function generateInputsUI() {
            addQuestion("Question1");
            //Row1 ******************************
            var row = 1;

            //addInputLabel("VegetationType", row, 5);
            //addInputEditor("VegetationType", row, 7);

            ////Row ******************************
            //++row;
            addInputLabel("StartDay", row, 5,5);
            addInputEditor("StartDay", row, 2,2);
            addInputEditor("StartMonth", row, 2,2);
            addInputEditor("Year", row, 3,3);
            //Row ******************************
            ++row;
            addInputLabel("DaysToMaturity", row, 5,5);
            addInputEditor("DaysToMaturity", row, 7,7);
            //Row ******************************
            ++row;
            addInputLabel("StartSoilWater", row, 5,5);
            addInputEditor("StartSoilWater", row, 7,7);
            //Row ******************************

            ++row;
            addInputLabel("WUE", row, 5,5);
            addInputEditor("WUE", row, 7,7);
            //Row ******************************
            ++row;
            addInputLabel("Threshold", row, 5,5);
            addInputEditor("Threshold", row, 7,7);
            ++row;


            addInputLabel("Location", row, 5,5);
            addInputEditor("Location", row, 7,7);
            //Row ******************************
            //++row;
            //addInputLabel("SoilType", row, 5);
            //addInputEditor("SoilType", row, 7);
            ////Row ******************************
            
            ++row;
            addInputLabel("YearsRange", row, 5,5);
            addInputEditor("YearsRange", row, 7,7);
            refeshPeriodSelector();
            //Change Events *********************
            $(".input-control").change(function() {
                if (updateInputValue(mydatasource, $(this))) {
                    var identifier =  $(this).attr("id");
                    refeshPeriodSelector();
                    if (identifier === "StartMonth") {
                        rebuildDaySelector("StartDay", "StartMonth");
                    }
                    
                    calculate(mydatasource, calculationsUrl);
                }
            });
        }

        function generateOutputsUI() {
            addOutput("Question1");
            addOutput("Answer1");
            addOutput("FireGauge1");
            addOutput("Chart1");
            addOutput("HowDoWeKnowThis");
            var bodyWidth = $("body").width();
            if (bodyWidth < 700) {
                $("#Chart1 .flotchart-canvas").css("height", "260px");
                $("#Chart2 .flotchart-canvas").css("height", "260px");
            }
        }

        function refeshPeriodSelector() {
            var inputs = mydatasource.InputValues;
            var startday = inputs.StartDay;
            var startmonth = inputs.StartMonth;
            var startyear = inputs.Year;
            var startdate = new Date(startyear, startmonth - 1, startday);
            var currentdate = new Date();
            var diff = daydiff(currentdate, startdate);
            var firstyear = getFirstValidYear(inputs);
            var lastyear = currentdate.getYear() + 1900;
            if (diff > 1) {
                --lastyear;
            }
            if (inputs.Year < firstyear)
                inputs.Year = firstyear;
            if (inputs.Year > lastyear)
                inputs.Year = lastyear;
            rebuildPeriodSelector("Year", inputs.Year, firstyear, lastyear);
        }

        function getFirstValidYear(inputs) {
            switch (Number(inputs.YearsRange)) {
            case 0 /*YearsRangeType.From1900ToCurrent*/:
                return 1900;
            case 1 /*YearsRangeType.From1910ToCurrent*/:
                return 1910;
            case 2 /*YearsRangeType.From1920ToCurrent*/:
                return 1920;
            case 3 /*YearsRangeType.From1930ToCurrent*/:
                return 1930;
            case 4 /*YearsRangeType.From1940ToCurrent*/:
                return 1940;
            case 5 /*YearsRangeType.From1950ToCurrent*/:
                return 1950;
            case 6 /*YearsRangeType.From1960ToCurrent*/:
                return 1960;
            case 7 /*YearsRangeType.From1970ToCurrent*/:
                return 1970;
            case 8 /*YearsRangeType.From1980ToCurrent*/:
                return 1980;
            case 9 /*YearsRangeType.From1990ToCurrent*/:
                return 1990;
            case 10 /*YearsRangeType.From2000ToCurrent*/:
                return 2000;
            case 11 /*YearsRangeType.From1900To1950*/:
                return 1900;
            case 12 /*YearsRangeType.From1900To1970*/:
                return 1900;
            }
            return 1970;
        }

        function daydiff(first, second) {
            return Math.round((second - first) / (1000 * 60 * 60 * 24));
        }
    </script>


    <script type="text/javascript">
        $(function() {
            var $metismenu = $("#analysis-metismenu");
            var $selectedli = $(".analysis-a9");
            var $parentlis = $selectedli.parents("li");
            $selectedli.addClass("active");
            $selectedli.addClass("selected");
            $parentlis.addClass("active");
            $metismenu.metisMenu();
        });
        window.addEventListener('resize', posFix, false);

        function posFix() {
            generateOutputsUI();

        }

        function sendFeedback() {
            openFeedbackDialog("PotentialYield", "0ce5898f-ea6c-4832-b754-12dd6273bff9");
        }

        function getHelp() {
            $('.ajax_spinner').show();
            $.getJSON("/Analyses/GetHelp",null, function (results) {
                if (results.success) {
                    $("#help-title").text(results.title);
                    if (results.image && results.image.length > 0) {
                        $("#help-image").attr('src', results.image);

                    } else {
                        $("#help-image").hide();
                    }
                    $("#help-content").html(results.content);
                    $('.ajax_spinner').hide();
                    openNav();
                } else {
                    $('.ajax_spinner').hide();
                    custom_alert("Sorry - couldn't find any help for this analysis.", "Help not at hand...");
                }
                
            });
        }
        function openNav() {
           
            //$("#myNav").css("width", "100%");
            $("#myNav").css("left", "0");
        }

        function closeHelp() {
            //$("#myNav").css("width","0%");
            $("#myNav").css("left", "-100%");
        }

        function custom_alert(message, title) {
            if (!title)
                title = 'Alert';

            if (!message)
                message = 'No Message to Display.';

            $('<div></div>').html(message).dialog({
                title: title,
                resizable: false,
                modal: true,
                buttons: {
                    'Ok': function () {
                        $(this).dialog('close');
                    }
                }
            });
        }

    </script>


