            var calculationsUrl = '/A04_HowWetN/Calculate';

            $(function() {
                initLocationSelector();
                generateInputsUI();
                generateOutputsUI();
                calculate(mydatasource, calculationsUrl);
            });

            function generateInputsUI() {
                addQuestion("Question1");
                var row = 1;
                //Row1 ******************************
                addInputLabel("FallowLength", row, 1,3);
                addInputEditor("FallowLength", row, 3,9);
                addInputLabel("Location", row, 1,3);
                addInputEditor("Location", row, 3,9);

                addInputLabel("StartSoilMoisture", row, 1,3);
                addInputEditor("StartSoilMoisture", row, 3,9);
                //Row2 ******************************
                ++row;
                addInputLabel("SoilType", row, 1,3);
                addInputEditor("SoilType", row, 3,9);

                addInputLabel("AvgCover", row, 1,3);
                addInputEditor("AvgCover", row, 3,9);

                addInputLabel("YearsRange", row, 1,3);
                addInputEditor("YearsRange", row, 3,9);

                //Change Events *********************
                $(".input-control").change(function() {
                   
                    if (updateInputValue(mydatasource, $(this))) {
                        
                        calculate(mydatasource, calculationsUrl);

                    } else {
                        var identifier = $(this).attr("id");
                 
                       if (identifier === "Location") {
                           reloadCombatibleSoilsList();
                       }
                    }
                });
            }

            function generateOutputsUI() {
                addOutput("Question1");
                addOutput("Answer1");
                addOutput("FireGauge1");
                addOutput("Table1");
                addOutput("Chart1");
                addOutput("Question2");
                //addOutput("Answer2");
                //addOutput("FireGauge2");
                addOutput("Chart2");
                addOutput("HowDoWeKnowThis");
                var bodyWidth = $("body").width();
                if (bodyWidth < 700) {
                    $("#Chart1 .flotchart-canvas").css("height", "260px");
                    $("#Chart2 .flotchart-canvas").css("height", "260px");
                }
            }


            
            function reloadCombatibleSoilsList() {
                var url = "/A04_HowWetN/GetFilteredSoils/"+mydatasource.InputValues.Location;
                $.getJSON(url, null, function (results) {
                    //populate dropdown with filtered reports.
                    var $soilSelector = $("#SoilType");

                    $soilSelector.empty();
                    $(results.soils).each(function (i, result) {
                        $soilSelector.append($("<option>", { value: result.key, html: result.value }));
                    });
                });

            }

        </script>

    
    <script type="text/javascript">
        $(function() {
            var $metismenu = $("#analysis-metismenu");
            var $selectedli = $(".analysis-a4");
            var $parentlis = $selectedli.parents("li");
            $selectedli.addClass("active");
            $selectedli.addClass("selected");
            $parentlis.addClass("active");
            $metismenu.metisMenu();
        });
        window.addEventListener('resize', posFix, false);

        function posFix() {
            generateOutputsUI();

        }

        function sendFeedback() {
            openFeedbackDialog("HowWetN", "ce0ab6a5-1f1a-4088-a4fe-e1bf211b43fa");
        }

        function getHelp() {
            $('.ajax_spinner').show();
            $.getJSON("/Analyses/GetHelp",null, function (results) {
                if (results.success) {
                    $("#help-title").text(results.title);
                    if (results.image && results.image.length > 0) {
                        $("#help-image").attr('src', results.image);

                    } else {
                        $("#help-image").hide();
                    }
                    $("#help-content").html(results.content);
                    $('.ajax_spinner').hide();
                    openNav();
                } else {
                    $('.ajax_spinner').hide();
                    custom_alert("Sorry - couldn't find any help for this analysis.", "Help not at hand...");
                }
                
            });
        }
        function openNav() {
           
            //$("#myNav").css("width", "100%");
            $("#myNav").css("left", "0");
        }

        function closeHelp() {
            //$("#myNav").css("width","0%");
            $("#myNav").css("left", "-100%");
        }

        function custom_alert(message, title) {
            if (!title)
                title = 'Alert';

            if (!message)
                message = 'No Message to Display.';

            $('<div></div>').html(message).dialog({
                title: title,
                resizable: false,
                modal: true,
                buttons: {
                    'Ok': function () {
                        $(this).dialog('close');
                    }
                }
            });
        }

    </script>


    <script type="text/javascript">
    $(function () {
        var $selectedmainli = $(".main-analyses");
        
        $selectedmainli.addClass("active");
        //if($selectedsubmenuli) {
        //    $selectedsubmenuli.addClass("active");
        //}
    });
    </script>
   

