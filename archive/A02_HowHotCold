        var calculationsUrl = '/A02_HowHotCold/Calculate';

        $(function() {
            if (!mydatasource.InputValues.ShowConditionsAtDate)
                $(".firegauge-outputs").hide();
            initLocationSelector();
            hideShowEditorPanels();
            generateInputsUI();
            generateOutputsUI();
            calculate(mydatasource, calculationsUrl);
        });

        function generateInputsUI() {
            $("#analysis-inputs").empty();
            addQuestion("Question1");
            var row = 0;
            var stressType = Number(mydatasource.InputValues.StressType);
            if (mydatasource.InputValues.ShowConditionsAtDate === true) {


                if (stressType !== 2) {

                    //Row1 ******************************
                    ++row;
                    addInputEditor("StressType", row, 4, 4);
                    addInputLabel("Location", row, 1, 1, "right");
                    addInputEditor("Location", row, 7, 7);

                    //Row2 ******************************
                    ++row;
                    addInputLabel("TargetDateIndex", row, 7, 7);
                    addInputEditor("TargetDateIndex", row, 5, 5);

                    //Row3 ******************************
                    ++row;
                    addInputLabel("HeatStressThreshold", row, 7, 7);
                    addInputEditor("HeatStressThreshold", row, 5, 5);
                    addInputLabel("ColdStressThreshold", row, 7, 7);
                    addInputEditor("ColdStressThreshold", row, 5, 5);
                    //Row4 ******************************
                    ++row;
                    addInputLabel("Risk", row, 7, 7);
                    addInputEditor("Risk", row, 5, 5);
                    //Row5 ******************************
                    ++row;
                    addInputLabel("YearsRange", row, 7, 7);
                    addInputEditor("YearsRange", row, 5, 5);
                } else {
                    //Row1 ******************************
                    ++row;
                    
                    addInputEditor("StressType", row, 3, 4);
                    addInputLabel("Location", row, 1, 1);
                    addInputEditor("Location", row, 3, 7);
                    addInputLabel("TargetDateIndex", row, 3, 7);
                    addInputEditor("TargetDateIndex", row, 2, 5);
                  
                    //Row2 ******************************
                    ++row;
                    addInputLabel("HeatStressThreshold", row, 5, 7);
                    addInputEditor("HeatStressThreshold", row, 2, 5);
                    addInputLabel("ColdStressThreshold", row, 3, 7);
                    addInputEditor("ColdStressThreshold", row, 2, 5);
                    //Row2 ******************************
                    ++row;
                    addInputLabel("Risk", row, 5, 7);
                    addInputEditor("Risk", row, 2, 5);
                    addInputLabel("YearsRange", row, 3, 7);
                    addInputEditor("YearsRange", row, 2, 5);
                }
            } else {
                ++row;
                addInputLabel("StressType", row, 3, 1);
                addInputEditor("StressType", row, 3, 11);
                addInputLabel("Location", row, 3, 1);
                addInputEditor("Location", row, 3, 11);

               
                //Row2 ******************************
                ++row;
                addInputLabel("HeatStressThreshold", row, 3, 7);
                addInputEditor("HeatStressThreshold", row, 3, 5);
                addInputLabel("ColdStressThreshold", row, 3, 7);
                addInputEditor("ColdStressThreshold", row, 3, 5);
                //Row2 ******************************
                ++row;
                addInputLabel("Risk", row, 3, 7);
                addInputEditor("Risk", row, 3, 5);
                addInputLabel("YearsRange", row,3, 7);
                addInputEditor("YearsRange", row, 3, 5);
            }
            // **********************************
            hideShowThresholdEditors();
            //Change Events *********************
            $(".input-control").change(function() {
               
                if (updateInputValue(mydatasource, $(this))) {
                    hideShowEditorPanels();
                    generateInputsUI();
                    calculate(mydatasource, calculationsUrl);
                }
            });
        }

        function generateOutputsUI() {

            addOutput("Question1");
            addOutput("Answer1");
            if (mydatasource.InputValues.ShowConditionsAtDate) {
                addOutput("FireGauge1");
                if (Number(mydatasource.InputValues.StressType) === 2) {
                    addOutput("Answer2");
                    addOutput("FireGauge2");
                }
            }
            addOutput("Chart1");
            addOutput("HowDoWeKnowThis");
            var documentWidth = $(document).width();
            if (documentWidth < 700) {
                $("#Chart1 .flotchart-canvas").css("height", "260px");
            }
        }

        function hideShowThresholdEditors() {
            var stressType = Number(mydatasource.InputValues.StressType);
            hideShowElement("HeatStressThreshold", stressType !== 1);
            hideShowElement("ColdStressThreshold", stressType !== 0);
        }

        function hideShowEditorPanels() {

            var stressType = Number(mydatasource.InputValues.StressType);

            if (mydatasource.InputValues.ShowConditionsAtDate && stressType !== 2) {
                //Hide Presentation 2
                $("#presentation2").hide();
                $("#presentation2 .question-panel").empty();
                $("#presentation2 .firegauge-container").empty();
                $("#presentation2 .answer").empty();
                $("#presentation2 .question-panel").attr("id", "analysis-inputs-off");
                $("#presentation2 #FireGauge1").attr("id", "FireGauge1-off");
                $("#presentation2 #Answer1").attr("id", "Answer1-off");

                //Activate Presentation 1
                $("#presentation1").show();
                $("#presentation1 .question-panel").attr("id", "analysis-inputs");
                $("#presentation1 #Answer1-off").attr("id", "Answer1");
                $("#presentation1 #FireGauge1-off").attr("id", "FireGauge1");


            } else {
                //Hide Presentation 1
                $("#presentation1").hide();
                $("#presentation1 .question-panel").empty();
                $("#presentation1 .firegauge-container").empty();
                $("#presentation1 .answer").empty();
                $("#presentation1 .question-panel").attr("id", "analysis-inputs-off");
                $("#presentation1 #Answer1").attr("id", "Answer1-off");
                $("#presentation1 #FireGauge1").attr("id", "FireGauge1-off");

                //Activate Presentation 2
                $("#presentation2").show();
                $("#presentation2 .question-panel").attr("id", "analysis-inputs");
                $("#presentation2 #Answer1-off").attr("id", "Answer1");
                $("#presentation2 #FireGauge1-off").attr("id", "FireGauge1");


            }

            generateInputsUI();
        }


    </script>


    <script type="text/javascript">
        $(function() {
            var $metismenu = $("#analysis-metismenu");
            var $selectedli = $(".analysis-a2");
            var $parentlis = $selectedli.parents("li");
            $selectedli.addClass("active");
            $selectedli.addClass("selected");
            $parentlis.addClass("active");
            $metismenu.metisMenu();
        });
        window.addEventListener('resize', posFix, false);

        function posFix() {
            generateOutputsUI();

        }

        function sendFeedback() {
            openFeedbackDialog("HowHotCold", "63afa4ce-cc89-443e-a805-9aa3ae5c548e");
        }

        function getHelp() {
            $('.ajax_spinner').show();
            $.getJSON("/Analyses/GetHelp",null, function (results) {
                if (results.success) {
                    $("#help-title").text(results.title);
                    if (results.image && results.image.length > 0) {
                        $("#help-image").attr('src', results.image);

                    } else {
                        $("#help-image").hide();
                    }
                    $("#help-content").html(results.content);
                    $('.ajax_spinner').hide();
                    openNav();
                } else {
                    $('.ajax_spinner').hide();
                    custom_alert("Sorry - couldn't find any help for this analysis.", "Help not at hand...");
                }
                
            });
        }
        function openNav() {
           
            //$("#myNav").css("width", "100%");
            $("#myNav").css("left", "0");
        }

        function closeHelp() {
            //$("#myNav").css("width","0%");
            $("#myNav").css("left", "-100%");
        }

        function custom_alert(message, title) {
            if (!title)
                title = 'Alert';

            if (!message)
                message = 'No Message to Display.';

            $('<div></div>').html(message).dialog({
                title: title,
                resizable: false,
                modal: true,
                buttons: {
                    'Ok': function () {
                        $(this).dialog('close');
                    }
                }
            });
        }

    </script>


    <script type="text/javascript">
    $(function () {
        var $selectedmainli = $(".main-analyses");
        
        $selectedmainli.addClass("active");
        //if($selectedsubmenuli) {
        //    $selectedsubmenuli.addClass("active");
        //}
    });
    </script>
   

</body>
</html>
